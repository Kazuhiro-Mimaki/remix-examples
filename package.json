{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:eslint": "eslint --max-warnings 0 \"**/*.ts*\"",
    "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,md}\"",
    "fix:eslint": "eslint --fix --max-warnings 0 \"**/*.ts*\"",
    "fix:prettier": "prettier --write \"**/*.{js,ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "@turbo/gen": "^1.9.7",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.15.0",
  "name": "remix-examples"
}
